{
  "branches":[
     "master",
     "next",
     {
        "name":"beta",
        "prerelease":true
     }
  ],
  "plugins":[
     [
        "@semantic-release/commit-analyzer",
        {
           "preset":"conventionalcommits",
           "releaseRules":[
              {
                 "breaking":true,
                 "release":"major"
              },
              {
                 "type":"feat",
                 "release":"minor"
              },
              {
                 "type":"fix",
                 "release":"patch"
              },
              {
                 "type":"perf",
                 "release":"patch"
              },
              {
                 "type":"revert",
                 "release":"patch"
              },
              {
                 "type":"docs",
                 "release":"minor"
              },
              {
                 "type":"style",
                 "release":"patch"
              },
              {
                 "type":"refactor",
                 "release":"patch"
              },
              {
                 "type":"test",
                 "release":"patch"
              },
              {
                 "type":"build",
                 "release":"patch"
              },
              {
                 "type":"ci",
                 "release":"patch"
              },
              {
                 "type":"chore",
                 "release":false
              }
           ]
        }
     ],
     "@semantic-release/release-notes-generator",
     "@semantic-release/changelog",
     "@semantic-release/github",
     [
        "semantic-release-cargo",
        {
           "allFeatures":true,
           "check":true,
           "publishArgs":[
              "--no-verify"
           ]
        }
     ],
     [
        "@semantic-release/git",
        {
           "assets":[
              "Cargo.toml"
           ],
           "message":"chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
     ]
  ]
}